{
  "name": "contact-flux",
  "version": "0.0.1",
  "description": "Example Flux contact manager",
  "repository": "https://github.com/p0o/Contact-Manager",
  "main": "app/js/app.js",
  "dependencies": {
    "flux": "^2.0.1",
    "keymirror": "~0.1.0",
    "object-assign": "^1.0.0",
    "react": "^0.12.0"
  },
  "devDependencies": {
    "browserify": "^6.2.0",
    "envify": "^3.0.0",
    "reactify": "^0.15.2",
    "uglify-js": "~2.4.15",
    "watchify": "^2.1.1"
  },
  "scripts": {
    "start": "watchify -o app/js/bundle.js -v -d app/js/app.js",
    "build": "browserify . -t [envify --NODE_ENV production] | uglifyjs -cm > js/bundle.min.js"
  },
  "author": "Pooria Atarzadeh",
  "browserify": {
    "transform": [
      "reactify",
      "envify"
    ]
  }
}
